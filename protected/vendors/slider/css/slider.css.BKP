#wrapper {
	/*padding-top: 49px;*/
	margin-top:93px;
}

.inner {
	width: 1180px;
	margin: 0 auto;
	position: relative;
}
.inner.shaded {
	background: #fafafa;
}


/* Hero banners */

.hero {
	width: 100%;
	height: 750px;
	position: relative;
	overflow: hidden;
	padding-bottom: 38px;
	margin-bottom: -38px;
	clear:both;
}
.hero .rotator {
	height: 750px;
	overflow: hidden;
}
.hero .hero-link {
	position: relative !important;
	background-color: #000000;
	width:980px;
	min-width: 980px;
	max-width:980;
}
.hero .hero-inner-link {
	position: absolute;
	bottom: 38px;
	left: 0;
	width: 100%;
	height: 96%;
	z-index: 50;
}

.hero .hero-caption-container {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	text-align: center;
}
.hero .hero-caption {
	/*background: rgba(63, 54, 47, 0.5);*/
	color: #ffffff;
	font-size: 40px;
	display: inline-block;
	margin: 33% auto 0 auto;
	font-size: 40px;
	padding: 14px 22px;
	max-width: 80%;
	white-space: normal;
	text-decoration: none;
}

.hero .hero-sub-caption {
	display: block;
	margin: 0 auto;
	font-size: 30px;
	font-style: italic;
	color: #ffffff;
	padding: 8px 22px;
	max-width: 80%;
}
